/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background: #efefef;
}
.my-top{
  width: 100%;
  /* height:400rpx; */
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
  color: #fff;
  font-size:60rpx; 
  background: #1b82d2;
}
.play-center{
  display:flex;
  justify-content: center;
  align-items: center;
}
.right_arrows {
  width: 20rpx;
  height: 20rpx;
  border-top: 1rpx solid #c3c8d6;
  border-right: 1rpx solid #c3c8d6;
  transform: rotate(45deg);
  margin-right: 10rpx;
  }
.login-btn{
  background: #1b82d2;
  padding:20rpx;
  border-radius: 50rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 80%;
  font-size: 30rpx;
  margin-top: 40rpx;
}
.play-around{
  display: flex;
  justify-content: space-around;
  align-items: center;
}