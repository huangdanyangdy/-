
.login-box{
  /* background: #fff; */
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin: 200rpx 0;
  padding: 30rpx;
  padding-bottom: 50rpx;
}
.input-box{
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border: 1rpx solid #ddd;
  border-radius: 50rpx;
  padding: 10rpx ;
  padding-left: 30rpx;
  font-size: 30rpx;
  margin: 20rpx;
  width:80%;
}
.input-box input{
  width:85%;
}
.input-box image{
  width: 50rpx;
  height:50rpx;
}

.copy-right{
  width: 100%;
  color:#999;
  font-size: 22rpx;
  position: fixed;
  bottom: 30rpx;
}