page{
  position: relative;
}
.check-tit{
  padding: 20rpx;
  font-size: 30rpx;
  font-weight: 500;
  color:#666;
  box-sizing: border-box;
}
.com-name{
  line-height: 40rpx;
}
.btn-box{
  display: flex;
  align-items:center;
  justify-content: flex-end;
}
.reset{
  width:120rpx;
  display: flex;
  align-items:center;
  justify-content: center;
  background: #1b82d2;
  padding:10rpx 20rpx;
  margin-left: 20rpx;
  color: #fff;
  border-radius: 20rpx;
  box-sizing: border-box;
}
.check-list{
  background: #fff;
  padding: 20rpx;
  margin-bottom: 20rpx;
  font-size: 32rpx;
  line-height: 60rpx;
}
.radiobox{
  display: flex;
  padding: 10rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  width: 100%;
  box-sizing: border-box;
}
.my-table{
  width:100%;
  /* height:300rpx; */
  border:1rpx solid #666;
  box-sizing: border-box;


}
.tb-tit{
  width:100%;
  display: flex;
  background: #e8e8e8;
  border-bottom:1rpx solid #666;
  box-sizing: border-box;
}
.tit-1{
  width:20%;
  border-right:1rpx solid #666;
  padding: 10rpx;

}
.tit-2{
  width:40%;
  border-right:1rpx solid #666;
  padding: 10rpx;
}

.tit-3{
  width:40%;
  padding: 10rpx;
}
.addBtn{
padding: 10rpx;

  width: 50%;
  background: #e8e8e8;
}
.addBtn:first-of-type{
 border-right:1rpx solid #666;
}

.tb-content{
  width:100%;
  display: flex;
  background: #fff;
  border-bottom:1rpx solid #666;
  box-sizing: border-box;
}
.my-img-box{
  /* width: 100%; */
  display: flex;
  align-items: center;
  overflow: scroll;
}
.img-items{
  width: 150rpx;
  height: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin:10rpx;
}
.my-img-box image{
  width: 150rpx;
  height: 150rpx;
}
.mask{
  /* width: 100%;
  height: 100%; */
  /* padding-bottom:20rpx; */
  /* position: fixed; */
  /* left: 0;
  top:0rpx; */
  /* z-index: 9; */
  /* background:rgba(0, 0, 0, 0.5); */
  /* box-sizing: border-box; */
}
.my-toast{
  width: 100%;
  height: 100%;
  /* padding-bottom:20rpx; */
  position: fixed;
  /* margin: auto; */
  z-index: 9;
  background:rgba(0,0,0,0.6);
  left: 0;
  top:0rpx;
  box-sizing: border-box;
}
.my-toast image{
  width: 100%;
  height: 100%;
}
.cameraPosition{
  width: 100%;
  height: 100%;
  padding-bottom:20rpx;
  position: fixed;
  left: 0;
  top:0rpx;
  z-index: 99;
  background: #000;
  box-sizing: border-box;
}
.camera_btn_box{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 15rpx;
  position: absolute;
  bottom:20rpx;
}

.my-btn{
  width: 40%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.save_btn{
  width: 100%;
  padding: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
  color: #fff;
  background:#1b82d2;
  font-size: 32rpx;
  box-sizing: border-box;
  border-radius:50rpx; 
}

