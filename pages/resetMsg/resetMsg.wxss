.my_radio{
  display:flex;
  flex-direction:column;
  flex-wrap:no-wrap;
  line-height: 70rpx;
}
.save_btn{
  background: #1b82d2;
  color: #fff;
  width: 80%;
  /* padding: 30rpx; */
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
  border-radius: 50rpx;
}

.copy-right{
  width: 100%;
  color:#999;
  font-size: 22rpx;
  margin: 100rpx 0 20rpx 0;
}

